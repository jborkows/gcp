{
  "private": true,
  "main": "firebaseFunctions.js",
  "scripts": {
    "dev": "next src/",
    "build": "next build src/",
    "start": "concurrently --kill-others \"npm run dev\"  \"FIREBASE_AUTH_EMULATOR_HOST='localhost:9099' firebase emulators:start  --import=./dir \"",
    "serve": "npm run build && firebase emulators:start --only functions,hosting",
    "serve-all": "npm run build && FIREBASE_AUTH_EMULATOR_HOST='localhost:9099' firebase emulators:start  --import=./dir --export-on-exit",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions,hosting",
    "export": "npm run build && next export",
    "deploy-static": "npm run export && firebase deploy --only hosting",
    "logs": "firebase functions:log",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
    "test": "firebase emulators:exec 'jest'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/react-redux": "^7.1.24",
    "axios": "^0.26.0",
    "firebase": "^9.6.8",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1",
    "next": "12.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^6.0.0",
    "react-redux": "^8.0.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^2.0.2",
    "@next/eslint-plugin-next": "^12.1.2",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.3.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.3"
  }
}
